{
  "name": "idealeware",
  "version": "4.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://wwww.idealeware.com.br"
  },
  "scripts": {
    "gulp": "gulp",
    "ng": "ng",
    "start": "ng serve",
    "start:hmg": "ng serve -c homologation",
    "build": "ng build",
    "lint": "ng lint idealeware",
    "compodoc": "compodoc -p tsconfig.json",
    "build:client-and-server-bundles": "ng build --prod --build-optimizer && ng run idealeware:server:production",
    "build:client-and-server-bundles:hmg": "ng build -c homologation --prod --build-optimizer && ng run idealeware:server:homologation",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run gulp clean && npm run build:client-and-server-bundles && npm run webpack:server && npm run gulp images && npm run gulp copy-google-ownership",
    "build:ssr:hmg": "npm run gulp clean && npm run build:client-and-server-bundles:hmg && npm run webpack:server && npm run gulp images && npm run gulp copy-google-ownership",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jquery-mask-plugin": "^1.14.15",
    "jquery-zoom": "^1.7.21",
    "ngx-cookie-service": "^1.0.10",
    "ngx-slick": "^0.2.1",
    "owl.carousel": "^2.3.4",
    "preboot": "^6.0.0-beta.6",
    "rxjs": "6.2.2",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^6.11.4",
    "toastr": "^2.1.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@compodoc/compodoc": "^1.1.3",
    "@types/node": "^8.0.30",
    "codelyzer": "^4.0.2",
    "cpy-cli": "^1.0.1",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-imagemin": "^4.1.0",
    "http-server": "^0.10.0",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^4.2.0",
    "tslint": "^5.7.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.0"
  }
}
